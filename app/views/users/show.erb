<h1>Hello <%=@user.username%></h1><br>

<p><% if flash.has?(:message) %>
  <%= flash[:message] %>
<% end %></p>

<div class="row">
  <%if self.is_logged_in?(session) && session[:id] == self.current_user(session).id%>

    <nav aria-label="breadcrumb">
      <ol class="breadcrumb">
        <li class="breadcrumb-item active" aria-current="page"><a href="/sounds/new">Add New Sound</a></a></li>
      </ol>
    </nav>

    </div>
      <h3>Current Sounds</h3>
      <div class="row">
        <div class="col-sm-4">
      <%@sounds.each do |sound|%>
        <%slug = sound.slug%>
      <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item active" aria-current="page"><a href="/sounds/<%=slug%>"><%=sound.name%></a></li>
        </ol>
      </nav>
      <%end%>

      <%else%>


  <%end%><br>
        </div>
      </div>

<div class="row">
  <p>Click "Logout" to logout.</p><br>
  <form action="/logout" method="post">
    <input type="submit" value="Logout">
  </form>
</div>
