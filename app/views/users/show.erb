<h1>Hello <%=@user.username%></h1><br>
<%if self.is_logged_in?(session) && session[:id] == self.current_user(session).id%>

  <a href="/sounds/new">Add New Sound</a><br>
  <h3>Current Sounds</h3>

  <%@user.sounds.each do |sound|%>
    <%slug = sound.slug%>
    <a href="/sounds/<%=slug%>"><%=Sound.find_by_slug(slug).name%></a><br>
<%else%>

<%end%>

<%end%><br>

<p>Click "Logout" to logout.</>
  <form action="/logout" method="post">
    <input type="submit" value="Logout">
  </form>
